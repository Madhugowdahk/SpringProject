package Do;

public class ChatGpt {
public static void main(String[] args) {
	int n = 10;
    int a = 0, b = 1;
	for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            System.out.print(a + " ");
        }
        System.out.println();
        int sum = a + b;
        a = b;
        b = sum;
    }
}
}
