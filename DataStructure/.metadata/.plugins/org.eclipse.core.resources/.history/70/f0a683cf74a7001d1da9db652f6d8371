package dataandalgorithm;

public class MoveToZeroes {
	public static void movetozero(int[]a,int n)
	{
		
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				
			
			if(a[i]==0)
			{
				int temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
			
		}
		}
		for(int i=0;i<a.length;i++)
		{
			System.out.print(a[i]+" ");
		}
		
	}
	public static void main(String[] args) {
	int []a= {0,1,0,4,12};
	int n=a.length;
	movetozero(a,n);
}
}
